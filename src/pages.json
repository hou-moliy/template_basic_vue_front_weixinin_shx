{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^eb-(.*)": "@/components/eb-comp/eb-$1/eb-$1.vue",
      "^uni-(.*)": "@/components/uni-$1/uni-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/cl/index",
      "style": {
        "navigationBarTitleText": "彩铃",
        "enablePullDownRefresh": false,
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/mine/index",
      "style": {
        "navigationBarTitleText": "我的"
      }
    },
    {
      "path": "pages/mine/login",
      "style": {
        "navigationBarTitleText": "登录",
        "enablePullDownRefresh": false
      }
    }
  ],
  "subPackages": [
    {
      "root": "pagesCommon",
      "pages": [
        {
          "path": "templateConfig/index",
          "style": {
            "navigationBarTitleText": "公共的模板化配置页面",
            "enablePullDownRefresh": false
          }
        },
        {
          "path": "share/shareVideo",
          "style": {
            "navigationBarTitleText": "分享视频彩铃海报",
            "enablePullDownRefresh": false
          }
        },
        {
          "path": "policy/policy",
          "style": {
            "navigationBarTitleText": "",
            "enablePullDownRefresh": false
          }
        },
        {
          "path": "share/openShare",
          "style": {
            "navigationBarTitleText": "呼叫预览页面",
            "navigationStyle": "custom"
          }
        }
      ]
    },
    {
      "root": "pagesSpcl",
      "pages": [
        {
          "path": "ai/ai",
          "style": {
            "navigationBarTitleText": "AI换铃",
            "enablePullDownRefresh": false
          }
        },
        {
          "path": "spclLibrary/index",
          "style": {
            "navigationBarTitleText": "我的视彩",
            "enablePullDownRefresh": false
          }
        },
        {
          "path": "clVideo/clVideoSearch",
          "style": {
            "navigationBarTitleText": "视频彩铃搜索",
            "navigationStyle": "custom"
          }
        },
        {
          "path": "clVideo/clVdieoPlay",
          "style": {
            "navigationBarTitleText": "视频彩铃详情",
            "enablePullDownRefresh": false,
            "navigationStyle": "custom"
          }
        },
        {
          "path": "clVideo/clVideoPreview",
          "style": {
            "navigationBarTitleText": "呼叫预览",
            "enablePullDownRefresh": false,
            "navigationStyle": "custom"
          }
        }
      ]
    },
    {
      "root": "pagesYpcl",
      "pages": [
        {
          "path": "list/index",
          "style": {
            "navigationBarTitleText": "音频包页面示例",
            "enablePullDownRefresh": false
          }
        }
      ]
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "tabBar": {
    "color": "#909399",
    "selectedColor": "#303133",
    "backgroundColor": "#FFFFFF",
    "borderStyle": "black",
    "list": [
      {
        "pagePath": "pages/cl/index",
        "iconPath": "static/tabbar/cl_tabbar.png",
        "selectedIconPath": "static/tabbar/cl_active_tabbar.png",
        "text": "彩铃"
      },
      {
        "pagePath": "pages/mine/index",
        "iconPath": "static/tabbar/mine_tabbar.png",
        "selectedIconPath": "static/tabbar/mine_active_tabbar.png",
        "text": "我的"
      }
    ]
  }
}
